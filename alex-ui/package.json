{
  "name": "alex-ui",
  "version": "0.1.0",
  "description": "ALEX - Agile Light Easy Xpert Code Agent Terminal UI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "alex-ui": "dist/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "node dist/index.js",
    "demo": "tsx scripts/demo.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "ink": "^5.0.1",
    "zustand": "^4.4.7",
    "socket.io-client": "^4.7.4",
    "highlight.js": "^11.9.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "immer": "^10.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "tsx": "^4.6.0"
  },
  "keywords": [
    "ai",
    "agent",
    "terminal",
    "ui",
    "react",
    "ink",
    "typescript"
  ],
  "author": "ALEX Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}